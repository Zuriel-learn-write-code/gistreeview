# Example backend environment variables
# Copy to `.env` and fill actual values for your environment

# Database connection (Postgres)
# Format: postgresql://user:password@host:port/dbname
DATABASE_URL="postgresql://postgres:password@localhost:5432/gisdbtree"

# Application port
PORT=4000

# Cloudinary (optional) - fill if you use Cloudinary for uploads
CLOUDINARY_CLOUD_NAME=your_cloud_name
CLOUDINARY_API_KEY=your_api_key
CLOUDINARY_API_SECRET=your_api_secret

# Any other secrets (JWT, mail, etc.) can be added here
# JWT_SECRET=your_jwt_secret
